{
  "name": "unconfigurator",
  "type": "module",
  "version": "0.0.1-alpha.18",
  "packageManager": "pnpm@8.8.0",
  "description": "A web ui for managing frontend configurations",
  "author": "Naily <zero@naily.cc> (https://naily.cc)",
  "license": "MIT",
  "homepage": "https://github.com/configjs/unconfigurator",
  "repository": {
    "type": "git",
    "url": "git+github.com:configjs/unconfigurator.git",
    "directory": "packages/configurator"
  },
  "bugs": {
    "email": "zero@naily.cc",
    "url": "https://github.com/configjs/unconfigurator/issues"
  },
  "keywords": [
    "config",
    "configuration",
    "frontend",
    "ui",
    "vue"
  ],
  "exports": {
    "./config": {
      "import": "./dist/config.js",
      "require": "./dist/config.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "configure": "./bin/configure"
  },
  "files": [
    "dist",
    "frontend"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "chokidar './src/**/*' 'package.json' 'tsup.config.ts' 'tsconfig.json' -c 'tsup'",
    "prepublishOnly": "cp ../../README.md README.md && tsup"
  },
  "dependencies": {
    "@iconify/json": "2.2.214",
    "@naiable/utils": "0.0.6",
    "@types/js-yaml": "4.0.9",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitejs/plugin-vue-jsx": "4.0.0",
    "@vueuse/core": "10.10.0",
    "chalk": "5.3.0",
    "date-fns": "3.6.0",
    "importx": "0.3.5",
    "js-yaml": "4.1.0",
    "less": "4.2.0",
    "naive-ui": "2.38.2",
    "path-to-regexp": "6.2.2",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "socket.io": "4.7.5",
    "socket.io-client": "4.7.5",
    "unocss": "0.60.4",
    "unplugin-auto-import": "0.17.6",
    "unplugin-vue-components": "0.27.0",
    "vite": "5.2.12",
    "vite-plugin-vue-configurable-router": "0.0.9",
    "vite-plugin-vue-devtools": "7.2.1",
    "vue": "3.4.27",
    "vue-router": "4.3.2",
    "zod": "3.23.8",
    "zod-to-json-schema": "3.23.0"
  }
}
